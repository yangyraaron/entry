<UserControl x:Class="Entry.ModuleListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:entry="clr-namespace:Entry"
             xmlns:entry_controls="clr-namespace:Entry.Controls"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <entry_controls:PageFluidPanel x:Name="Modules"
                                   ItemHeight="65"
                                   ItemSource="{Binding Modules}"
                                   ItemWidth="65">
        <entry_controls:PageFluidPanel.ItemView>
            <entry:ModuleView />
        </entry_controls:PageFluidPanel.ItemView>
    </entry_controls:PageFluidPanel>
</UserControl>
